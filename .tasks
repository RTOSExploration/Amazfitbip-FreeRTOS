[+]
LLVM_DIR=/usr/lib/llvm-14
ARM_NONE_EABI_TOOLCHAIN_PATH=$HOME/purs3/gcc-arm-none-eabi-8-2018-q4-major

[project-build]
command=export WLLVM_OUTPUT_LEVEL=DEBUG LLVM_COMPILER=hybrid LLVM_COMPILER_PATH=$(+LLVM_DIR)/bin BINUTILS_TARGET_PREFIX=$(+ARM_NONE_EABI_TOOLCHAIN_PATH)/bin/arm-none-eabi PATH="$PATH:$(+ARM_NONE_EABI_TOOLCHAIN_PATH)/bin/" && make -j$(nproc) CC=wllvm && extract-bc bin/main.elf && $(+LLVM_DIR)/bin/llvm-dis bin/main.elf.bc
cwd=<root>
errorformat=%f:%l:%m
#output=terminal
#pos=bottom
save=2
